{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "cse341-code-student-L06-team-start",
      "program": "${workspaceFolder}/server.js", // Change this to your entry file
      "runtimeExecutable": "node",
      "args": [
        "--inspect"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      },
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ], // Adjust the path if your transpiled files are in a different folder
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}